@{
    ViewData["Title"] = "Importações";
}

<h1>Configurações gerais</h1>
<div class="row shadow-lg">
    <div class="col-md-6">
        <div class="d-flex">
            <form method="post" asp-action="ImportCities" enctype="multipart/form-data">
                <div class="input-group">
                    <input class="form-control" type="file" name="formFile">
                    <button type="submit" class="btn btn-secondary" data-bs-toggle="tooltip" data-bs-placement="right" data-bs-original-title="Nome Município|cod IBGE UF|Código Mun. IBGE" aria-describedby="tooltip152927">
                        Importar Cidades
                    </button>
                </div>
            </form>
        </div>
        <hr />
        <br />
        <h1>Outras importações</h1>
        <div class="form-group">
            <a asp-action="StatePop" class="btn btn-primary">Carregar estados</a>
        </div>
        <br />
    </div>
    <br /><hr />
</div>


