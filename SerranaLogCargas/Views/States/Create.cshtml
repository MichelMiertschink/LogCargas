@model SerranaLogCargas.Models.State;

@{
    ViewData["Title"] = "Estados";
}

<h2>@ViewData["Title"]</h2>
<hr/>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Acronym" class="control-label"></label>
                <input asp-for="Acronym" class="form-control"></input>
            </div>
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control"></input>
            </div>
            <br/>
            <div class="form-group">
                <input type="submit" value="Novo" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>